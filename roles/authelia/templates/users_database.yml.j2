###############################################################
#                         Users Database                      #
###############################################################

# This file can be used if you do not have an LDAP set up.

# List of users
users:
  {{ user.name }}:
    displayname: "{{ user.name }}"
    password: "{{ user.pass | string | password_hash('sha512', rounds=50000) }}"
    email: {{ user.email }}
    groups:
      - admins
...