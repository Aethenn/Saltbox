---
dns:
  proxied: no
  ipv4: yes
  ipv6: no
  zerossl: no
docker:
  json_driver: no
gpu:
  intel: yes
  nvidia: no
mounts:
  ipv4_only: no
system:
  timezone: auto
traefik:
  tls: no
  http: no
  metrics: no
  tracing: no
  hsts: no
  provider: cloudflare
  subdomains:
    dash: dash
    metrics: metrics
    jaeger: jaeger
  error_pages: no
